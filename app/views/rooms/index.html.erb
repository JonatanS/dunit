<p id="notice"><%= notice %></p>

<h1>Listing Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Revit</th>
      <th>Name</th>
      <th>Number</th>
      <th>Base finish</th>
      <th>Wall finish</th>
      <th>Ceiling finish</th>
      <th>Floor finish</th>
      <th>Occupancy</th>
      <th>Area</th>
      <th>Level name</th>
      <th>Height</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.revit_id %></td>
        <td><%= room.name %></td>
        <td><%= room.number %></td>
        <td><%= room.base_finish %></td>
        <td><%= room.wall_finish %></td>
        <td><%= room.ceiling_finish %></td>
        <td><%= room.floor_finish %></td>
        <td><%= room.occupancy %></td>
        <td><%= room.area %></td>
        <td><%= room.level_name %></td>
        <td><%= room.height %></td>
        <td><%= link_to 'Show', room %></td>
        <td><%= link_to 'Edit', edit_room_path(room) %></td>
        <td><%= link_to 'Destroy', room, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Room', new_room_path %>
